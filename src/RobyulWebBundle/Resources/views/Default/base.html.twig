                    <a class="dropdown-item" href="{{ path('robyulweb_showrankings_mixnine') }}">Girls{% if
                            app.request.attributes.get('_route') == 'robyulweb_showrankings_mixnine' %} <span
                            class="sr-only">(current)</span>{% endif %}</a>
