<!DOCTYPE html>
<html lang="en" {{ sonata_seo_html_attributes() }}>
<head {{ sonata_seo_head_attributes() }}>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" href="../../favicon.ico">

    {{ sonata_seo_title() }}
    {{ sonata_seo_metadatas() }}
    {{ sonata_seo_link_canonical() }}
    {{ sonata_seo_lang_alternates() }}

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.6/css/bootstrap.min.css" integrity="sha384-rwoIResjU2yc3z8GV/NPeZWAv56rSmLldC3R/AZzGRnGxQQKnKkoFVhFQhNUwEyJ" crossorigin="anonymous">

    {% stylesheets '@RobyulWebBundle/Resources/public/css/main.css' output='static/css/main.css' %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
    <link rel="stylesheet" type="text/css" href="//cdnjs.cloudflare.com/ajax/libs/cookieconsent2/3.0.3/cookieconsent.min.css" />
    <script src="//cdnjs.cloudflare.com/ajax/libs/cookieconsent2/3.0.3/cookieconsent.min.js"></script>
    <script data-cfasync="false">
        window.addEventListener("load", function(){
            window.cookieconsent.initialise({
                "palette": {
                    "popup": {
                        "background": "#252e39"
                    },
                    "button": {
                        "background": "#14a7d0"
                    }
                },
                "theme": "edgeless",
                "content": {
                    "href": "{{ path('robyulweb_default_privacypolicy') }}"
                }
            })});
    </script>
    {% block header %}{% endblock %}
</head>

<body>

<nav class="navbar navbar-toggleable-md navbar-inverse bg-inverse fixed-top">
    <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarsExampleDefault" aria-controls="navbarsExampleDefault" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
    </button>
    <a class="navbar-brand" href="{{ path('robyulweb_default_index') }}">
        {% image '@RobyulWebBundle/Resources/public/images/robyulblush.png' output='static/images/robyulblush.png' %}
        <img src="{{ asset_url }}" width="30" height="30" class="d-inline-block align-top" alt="robyul">
        {% endimage %}
        Robyul
    </a>

    <div class="collapse navbar-collapse" id="navbarsExampleDefault">
        <ul class="navbar-nav mr-auto">
            {# <li class="nav-item active">
                <a class="nav-link" href="#">Home <span class="sr-only">(current)</span></a>
            </li> #}
            <li class="nav-item{% if app.request.attributes.get('_route') == 'robyulweb_default_commands' %} active{% endif %}">
                <a class="nav-link" href="{{ path('robyulweb_default_commands') }}">Commands{% if
                    app.request.attributes.get('_route') == 'robyulweb_default_commands' %} <span class="sr-only">(current)</span>{% endif %}</a>
            </li>
            <li class="nav-item{% if app.request.attributes.get('_route') == 'robyulweb_default_profilebackgrounds' %} active{% endif %}">
                <a class="nav-link" href="{{ path('robyulweb_default_profilebackgrounds') }}">Backgrounds{% if
                    app.request.attributes.get('_route') == 'robyulweb_default_profilebackgrounds' %} <span class="sr-only">(current)</span>{% endif %}</a>
            </li>
            <li class="nav-item{% if app.request.attributes.get('_route') == 'robyulweb_default_statistics' %} active{% endif %}">
                <a class="nav-link" href="{{ path('robyulweb_default_statistics') }}">Statistics{% if
                    app.request.attributes.get('_route') == 'robyulweb_default_statistics' %} <span class="sr-only">(current)</span>{% endif %}</a>
            </li>
            <li class="nav-item{% if app.request.attributes.get('_route') == 'robyulweb_idolschool_index' %} active{% endif %}">
                <a class="nav-link" href="{{ path('robyulweb_idolschool_index') }}">Idol School{% if
                    app.request.attributes.get('_route') == 'robyulweb_idolschool_index' %} <span class="sr-only">(current)</span>{% endif %}</a>
            </li>
            {#<li class="nav-item">
                <a class="nav-link disabled" href="#">Disabled</a>
            </li>#}
            {#
            <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" href="http://example.com" id="dropdown01" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Dropdown</a>
                <div class="dropdown-menu" aria-labelledby="dropdown01">
                    <a class="dropdown-item" href="#">Action</a>
                    <a class="dropdown-item" href="#">Another action</a>
                    <a class="dropdown-item" href="#">Something else here</a>
                </div>
            </li> #}
        </ul>
        <span class="navbar-text">
            This website is in an early Alpha
        </span>
        <span class="navbar-text patreon-nav">
            <a href="https://www.patreon.com/sekl" target="_blank">{% image '@RobyulWebBundle/Resources/public/images/become_a_patron_button.png' output='static/images/become_a_patron_button.png' %}
                <img src="{{ asset_url }}" alt="patreon">
                {% endimage %}</a>
        </span>
        {#<form class="form-inline my-2 my-lg-0">
            <input class="form-control mr-sm-2" type="text" placeholder="Search">
            <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search</button>
        </form> #}
    </div>
</nav>

<div class="{% block containerclass %}container{% endblock %}">

    {% block content %}{% endblock %}

    <div class="footer">
        {% block footer %}
            <hr>
            &copy; Copyright 2017 by <a href="https://slmn.de/">Sebastian Winkler</a> - <a href="{{ path('robyulweb_default_imprint') }}">Impressum</a> - <a href="{{ path('robyulweb_default_privacypolicy') }}">Datenschutzerkl√§rung</a>.
        {% endblock %}
    </div>

</div><!-- /.container -->

<script src="https://code.jquery.com/jquery-3.1.1.slim.min.js" integrity="sha384-A7FZj7v+d/sdmMqp/nOQwliLvUsJfDHW+k9Omg/a/EheAdgtzNs3hpfag6Ed950n" crossorigin="anonymous"></script>
<script>window.jQuery || document.write('<script src="../../assets/js/vendor/jquery.min.js"><\/script>')</script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/tether/1.4.0/js/tether.min.js" integrity="sha384-DztdAPBWPRXSA/3eYEEUWrWCy7G5KFbe8fFjk5JAIxUYHKkDx6Qin1DkWx51bBrb" crossorigin="anonymous"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.6/js/bootstrap.min.js" integrity="sha384-vBWWzlZJ8ea9aCX4pEW3rVHjgjt7zpkNpZk+02D9phzyeVkE+jo0ieGizqPLForn" crossorigin="anonymous"></script>
<!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
{% javascripts
'@RobyulWebBundle/Resources/public/js/main.js'
'@RobyulWebBundle/Resources/public/js/ie10-viewport-bug-workaround.js' output='static/js/main.js' %}
<script src="{{ asset_url }}"></script>
{% endjavascripts %}
{% if ga_tracking_id != "" %}
<script type="text/javascript" data-cfasync="false">
    var gaProperty = '{{ ga_tracking_id }}';
    var disableStr = 'ga-disable-' + gaProperty;
    if (document.cookie.indexOf(disableStr + '=true') > -1) {
        window[disableStr] = true;
    }
    function gaOptout() {
        document.cookie = disableStr + '=true; expires=Thu, 31 Dec 2099 23:59:59 UTC';
        path='/';
        window[disableStr] = true;
    }
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
        (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
        m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');
    ga('create', gaProperty, 'auto');
    ga('require', 'linkid');
    ga('send', 'pageview');
</script>
{% endif %}
{% block scripts %}{% endblock %}
</body>
</html>
